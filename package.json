{
  "name": "remote-func",
  "version": "0.4.23",
  "description": "JavaScript as the query language for your API",
  "main": "client/index.js",
  "repository": "https://github.com/yosbelms/remote-func",
  "keywords": [
    "api",
    "graphql",
    "rest",
    "node",
    "microservice",
    "http",
    "server",
    "client"
  ],
  "scripts": {
    "test": "npx jasmine-ts",
    "coverage": "npm run build && npx nyc -e .ts jasmine-ts",
    "clean": "find . -type f \\( -name \"*.js\" -o -name \"*.js.map\" -o -name \"*.d.ts\" -o -name \"*.d.ts.map\" \\) | grep -v \"./node_modules\" | xargs rm",
    "build": "tsc -p ./tsconfig.nodejs.json && tsc -p ./tsconfig.browser.json",
    "build-umd": "browserify ./client/index.js -o ./umd.js -s RemoteFunc",
    "watch": "npm run clean && npx sane 'npm run build' --glob='*.ts'",
    "prepublishOnly": "npm run clean && npm run build && npm run build-umd && npm run test"
  },
  "bin": "./bin/remote-func.js",
  "author": "yosbelms",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@types/babel__core": "^7.1.7",
    "@types/deep-freeze": "^0.1.2",
    "@types/koa-compose": "^3.2.5",
    "@types/node": "^12.12.42",
    "@types/rimraf": "^3.0.0",
    "endent": "^1.4.1",
    "globby": "^11.0.0",
    "koa-compose": "^4.1.0",
    "make-dir": "^3.1.0",
    "mri": "^1.1.6",
    "p-defer": "^3.0.0",
    "p-settle": "^4.1.0",
    "rimraf": "^3.0.2",
    "terser": "^4.7.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/glob": "^7.1.1",
    "@types/jasmine": "^3.5.10",
    "@types/node-fetch": "^2.5.7",
    "browserify": "^16.5.1",
    "delay": "^4.3.0",
    "express": "^4.17.1",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "micro": "^9.3.4",
    "node-fetch": "^2.6.0",
    "nyc": "^15.0.1",
    "sane": "^4.1.0",
    "ts-node": "^8.10.1",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=10"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": false,
    "source-map-support": true
  }
}
